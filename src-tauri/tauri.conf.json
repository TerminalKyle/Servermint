{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "servermint",
  "version": "0.1.2",
  "identifier": "com.servermint.gg",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "ServerMint",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/servermint.png"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgwOEQ2RTM0MUVEOEY0NzUKUldSMTlOZ2VORzZOZ002TzYyZWp6bTdSQTRUaDVSbGhIblgycU0reUNld3hyejFQOUFWVTVVWkwK",
      "endpoints": [
        "https://releases.servermint.app/updates.json"
      ]
    }
  }
}